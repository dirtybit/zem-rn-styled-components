{
  "name": "React Native Styled Components",
  "description": "Zepling extension to generate DN type styled components",
  "version": "0.1.0",
  "options": [
    {
      "name": "Color format",
      "type": "picker",
      "id": "colorFormat",
      "submenu": true,
      "options": [
        {
          "name": "Default",
          "value": "default"
        },
        {
          "name": "HEX",
          "value": "hex"
        },
        {
          "name": "RGB",
          "value": "rgb"
        },
        {
          "name": "HSL",
          "value": "hsl"
        }
      ],
      "default": "default"
    },
    {
      "name": "Text base component",
      "type": "text",
      "id": "textBaseComponent",
      "default": ""
    },
    {
      "name": "Theme namespace for colors in styled components",
      "type": "text",
      "id": "colorThemePrefix",
      "default": ""
    },
    {
      "name": "Theme namespace for fonts in styled components",
      "type": "text",
      "id": "fontThemePrefix",
      "default": ""
    }
  ],
  "moduleURL": "./main.js",
  "readmeURL": "./README.md"
}