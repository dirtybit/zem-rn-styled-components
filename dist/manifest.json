{
  "name": "React Native Styled Components",
  "description": "Zeplin extension that generates glamorous type styled components for React Native.",
  "version": "0.1.0",
  "author": {
    "name": "Johan Kasperi",
    "email": "johan@kasperi.se",
    "url": "http://kspri.se"
  },
  "options": [
    {
      "name": "Color format",
      "type": "picker",
      "id": "colorFormat",
      "submenu": true,
      "options": [
        {
          "name": "Default",
          "value": "default"
        },
        {
          "name": "HEX",
          "value": "hex"
        },
        {
          "name": "RGB",
          "value": "rgb"
        },
        {
          "name": "HSL",
          "value": "hsl"
        }
      ],
      "default": "default"
    },
    {
      "name": "Add font scale property to texts",
      "type": "switch",
      "id": "fontScale",
      "default": false
    },
    {
      "name": "Text base component",
      "type": "text",
      "id": "textBaseComponent",
      "default": ""
    },
    {
      "name": "Theme namespace for colors in styled components",
      "type": "text",
      "id": "colorThemePrefix",
      "default": ""
    },
    {
      "name": "Theme namespace for fonts in styled components",
      "type": "text",
      "id": "fontThemePrefix",
      "default": ""
    },
    {
      "name": "Path to typography components",
      "type": "text",
      "id": "typographyComponentsPath",
      "default": "StyledComponents/Typography"
    },
    {
      "name": "Path to blur component",
      "type": "text",
      "id": "blurComponentPath",
      "default": ""
    }
  ],
  "moduleURL": "./main.485a1c72.js",
  "repository": "https://github.com/johankasperi/zem-rn-styled-components.git",
  "readmeURL": "./README.md"
}